{
	"env": {
		"browser": true,
		"es2021": true
	},
	"root": true,
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-mixed-spaces-and-tabs": 0,
		"react/jsx-key": "warn",
		"react/display-name": "warn",
		"no-unused-vars": "warn",
		"react/prop-types": 0
	}
}
